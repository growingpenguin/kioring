<?xml version="1.0" encoding="utf-8"?>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kioring_background2"
    tools:context=".MainActivity42">

    <Button
        android:id="@+id/before"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/before"
        android:layout_marginTop="130dp"
        android:layout_marginStart="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/coffee"
        android:layout_width="131dp"
        android:layout_height="50dp"
        android:text="주스"
        android:textStyle="bold"
        android:textSize="28dp"
        android:background="@drawable/round4"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:gravity="center"
        android:layout_marginTop="130dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="316dp"
        android:layout_height="525dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="#D9D9D9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/coffee"
        app:layout_constraintVertical_bias="0.241" />

    <Button
        android:id="@+id/apple_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/apple"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="사과 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <Button
        android:id="@+id/avocado_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/avocado"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="아보카도 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toEndOf="@+id/apple_btn"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <Button
        android:id="@+id/banana_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/banana"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="바나나 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toEndOf="@+id/avocado_btn"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <Button
        android:id="@+id/beet_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/beet"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="비트 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/apple_btn" />

    <Button
        android:id="@+id/carrot_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/carrot"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="당근 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toEndOf="@+id/beet_btn"
        app:layout_constraintTop_toBottomOf="@+id/avocado_btn" />

    <Button
        android:id="@+id/grapefruit_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/grapefruit"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="자몽 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toEndOf="@+id/carrot_btn"
        app:layout_constraintTop_toBottomOf="@+id/banana_btn" />

    <Button
        android:id="@+id/kiwi_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/kiwi"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="키위 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/beet_btn" />

    <Button
        android:id="@+id/orange_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/orange"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="오렌지 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toEndOf="@+id/kiwi_btn"
        app:layout_constraintTop_toBottomOf="@+id/carrot_btn" />

    <Button
        android:id="@+id/pineapple_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/pineapple"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="파인애플 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toEndOf="@+id/orange_btn"
        app:layout_constraintTop_toBottomOf="@+id/grapefruit_btn" />

    <Button
        android:id="@+id/pomegranate_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/pomegranate"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="석류 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/kiwi_btn" />

    <Button
        android:id="@+id/tomato_btn"
        android:layout_width="87dp"
        android:layout_height="112dp"
        android:layout_marginStart="13dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="@color/white"
        android:drawableTop="@drawable/tomato"
        android:fontFamily="@font/bold"
        android:paddingTop="20dp"
        android:text="토마토 주스"
        android:textSize="12dp"
        android:textStyle="bold"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toEndOf="@+id/pomegranate_btn"
        app:layout_constraintTop_toBottomOf="@+id/orange_btn" />

    <Button
        android:id="@+id/addmenu"
        android:layout_width="148dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/round1"
        android:backgroundTint="#ED5351"
        android:fontFamily="@font/oseongandhaneumb"
        android:text="다른 카테고리에서\n메뉴추가"
        android:textColor="#FEF390"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/button19"
        android:layout_width="148dp"
        android:layout_height="60dp"
        android:layout_marginLeft="5dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/round1"
        android:backgroundTint="#ED5351"
        android:fontFamily="@font/oseongandhaneumb"
        android:text="결제"
        android:textColor="#FEF390"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/addmenu"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/basket"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/img_1"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>